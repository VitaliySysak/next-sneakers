generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("POSTGRES_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Product {
    id Int @id @default(autoincrement())
    name String @unique
    imageUrl String
    price Int
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
